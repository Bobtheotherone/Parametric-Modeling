{
  "version": 2,
  "limits": {
    "max_calls_per_agent": 15,
    "quota_retry_attempts": 5,
    "max_total_calls": 150,
    "max_json_correction_attempts": 3
  },
  "fallback_order": ["codex", "claude"],
  "agents": {
    "codex": {
      "script": "bridge/agents/codex.sh",
      "role": "implementer",
      "supports_write_access": true,
      "quota_error_patterns": [
        "rate limit",
        "429",
        "quota"
      ]
    },
    "claude": {
      "script": "bridge/agents/claude.sh",
      "role": "reviewer",
      "supports_write_access": true,
      "quota_error_patterns": [
        "rate limit",
        "429",
        "quota"
      ]
    }
  },
  "weights": {
    "codex": 1.0,
    "claude": 1.0
  },
  "enable_agents": ["codex", "claude"],
  "allow_os_write": true
}
